{
    "meta": {
        "label": "Sankey: bipartite",
        "description": "Simple, one-layer Sankey chart",
        "icon": "icon-bar-chart"
    },

    "baseType": "STANDARD",
    "hasBackend": "false",
    "params": [
        {
            "name": "dataset",
            "type": "DATASET",
            "label": "Dataset",
            "description": "Must have columns 'from', 'to' and 'weight'",
            "mandatory": true,
            "canSelectForeign": true
        },
        {
            "name": "max_links",
            "type": "INT",
            "label": "Max number of links",
            "defaultValue": 12,
            "mandatory": false
        },
        {
            "name": "min_weight",
            "type": "INT",
            "label": "Min weight",
            "defaultValue": 0,
            "mandatory": false
        }
    ],
    "roles": [
        {"type": "DATASET", "targetParamsKey": "dataset"}
    ]
}
